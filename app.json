{
	"name": "SurenaBot",
	"description": "Customizable whatsapp bo",
"keywords": [
    "bot",
    "surena",
    "surenabot",
    "whatsapp",
    "whatsappbot",
    "wabot",
    "baileys",
    "md",
    "multi-device"
  ],
	"repository": "https://github.com/SURENABOT/SURENABOT-MD",
	"stack": "/home/container",
	"env": {
		"BOT_NAME": {
			"description": "Your whatsapp bot name",
			"required": false,
			"value": "SURENA"
		},
		"WM_MSG": {
			"description": "Messages footer text",
			"required": false,
			"value": "SURENA"
		},
		"STICKER_PACK": {
			"description": "Stickers package name",
			"required": false,
			"value": "SURENA"
		},
		"STICKER_AUTHOR": {
			"description": "Stickers author name",
			"required": false,
			"value": "BOT"
		}
	},
	"addons": [
		{
			"plan": "heroku-postgresql"
		}
	]
}
