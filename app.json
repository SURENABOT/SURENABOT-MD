{
	"name": "SurenaBot",
	"description": "Customizable whatsapp bo",
"keywords": [
    "bot",
    "surena",
    "surenabot",
    "whatsapp",
    "whatsappbot",
    "wabot",
    "baileys",
    "md",
    "multi-device"
  ],
	"repository": "https://github.com/SURENABOT/SURENABOT-MD",
	"stack": "container",
	"env": {
		"BOT_NAME": {
			"description": "Your whatsapp bot name",
			"required": false,
			"value": "false"
		},
		"WM_MSG": {
			"description": "Messages footer text",
			"required": false,
			"value": "false"
		},
		"STICKER_PACK": {
			"description": "Stickers package name",
			"required": false,
			"value": "false"
		},
		"STICKER_AUTHOR": {
			"description": "Stickers author name",
			"required": false,
			"value": "false"
		},
		"STICKER_PACKNAME": {
			"description": "sticker pack info",
			"required": true,
			"value": "❤️,LyFE"
		}
	},
	"addons": [
		{
			"plan": "heroku-postgresql"
		}
	]
}
