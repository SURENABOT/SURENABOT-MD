{
	"name": "SURENABOT",
	"description": "Customizable whatsapp bot",
	"keywords": [
          "bot",
          "surena",
          "surenabot",
          "whatsapp",
          "whatsappbot",
          "wabot",
          "baileys",
          "md",
          "multi-device"
	],
	"repository": "https://github.com/SURENABOT/SURENABOT-MD",
	"stack": "container",
	"env": {
		"ALWAYS_ONLINE": {
			"description": "show bot number last seen as online",
			"required": false,
			"value": "false"
		},
		"LOG_MSG": {
			"description": "Show whatsapp msgs in log",
			"required": false,
			"value": "false"
		}
	},
	"addons": [
		{
			"plan": "heroku-postgresql"
		}
	]
}
